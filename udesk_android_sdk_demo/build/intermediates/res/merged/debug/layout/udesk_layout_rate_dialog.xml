<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="372dp"
    android:layout_height="wrap_content"
    android:background="#F9F9F9"
    android:gravity="center_horizontal"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/udesk_rate_dialog_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginTop="24dp"
        android:gravity="start"
        android:text="@string/udesk_rate_dialog_title"
        android:textColor="#474343"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/udesk_rate_dialog_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:layout_marginBottom="12dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginTop="12dp"
        android:gravity="start"
        android:lineSpacingExtra="6dp"
        android:text="@string/udesk_rate_dialog_message"
        android:textColor="#969696" />

    <include
        layout="@layout/udesk_line_horizontal"
        android:background="@color/udesk_divider" />

    <TextView
        android:id="@+id/udesk_rate_btn_grade"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:layout_marginTop="12dp"
        android:padding="3dp"
        android:text="@string/udesk_rate_grade"
        android:textColor="#354a93"
        android:textSize="14sp" />

    <include
        layout="@layout/udesk_line_horizontal"
        android:background="@color/udesk_divider" />

    <TextView
        android:id="@+id/udesk_rate_btn_feedback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:layout_marginTop="12dp"
        android:padding="3dp"
        android:text="@string/udesk_rate_feedback"
        android:textColor="#354a93"
        android:textSize="14sp" />


<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/udesk_im_text_item_panel"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center_vertical"
    android:orientation="horizontal"
    android:paddingBottom="@dimen/udesk_im_item_padding_top_bottom"
    android:paddingLeft="@dimen/udesk_im_item_padding_left_right_min"
    android:paddingRight="@dimen/udesk_im_item_padding_left_right_max"
    android:paddingTop="@dimen/udesk_im_item_padding_top_bottom" >
    
    <include layout="@layout/udesk_im_avatar" />

    <TextView
        android:id="@+id/udesk_im_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:autoLink="all"
        android:gravity="center_vertical"
        android:linksClickable="true"
        android:paddingBottom="12dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="12dp"
        android:text="00000000000"
        android:textSize="16sp" />
    
    <include layout="@layout/udesk_im_send_state" />
    


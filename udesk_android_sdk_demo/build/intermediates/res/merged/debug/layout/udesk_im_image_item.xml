<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/udesk_im_image_item_panel"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/udesk_im_item_padding_top_bottom"
    android:paddingLeft="@dimen/udesk_im_item_padding_left_right_min"
    android:paddingRight="@dimen/udesk_im_item_padding_left_right_max"
    android:paddingTop="@dimen/udesk_im_item_padding_top_bottom" >

    <include layout="@layout/udesk_im_avatar" />

    <ImageView
        android:id="@+id/udesk_im_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:maxHeight="200dp"
        android:maxWidth="200dp"
        android:minHeight="75dp"
        android:minWidth="75dp"
        android:padding="1dp"
        android:scaleType="fitXY" />

    <include layout="@layout/udesk_im_send_state" />

